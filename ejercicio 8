#include <iostream>
#include <cmath>
#include <complex>
using namespace std;

void cuadratica(double a, double b, double c) {
    if(a == 0) {
        cout << "No es ecuacion cuadratica" << endl;
        return;
    }
    
    double discriminante = b*b - 4*a*c;
    
    if(discriminante > 0) {
        double x1 = (-b + sqrt(discriminante)) / (2*a);
        double x2 = (-b - sqrt(discriminante)) / (2*a);
        cout << "Dos raices reales: " << x1 << " y " << x2 << endl;
    }
    else if(discriminante == 0) {
        double x = -b / (2*a);
        cout << "Una raiz doble: " << x << endl;
    }
    else {
        double real = -b / (2*a);
        double imaginaria = sqrt(-discriminante) / (2*a);
        cout << "Dos raices complejas: " << real << " + " << imaginaria << "i y " 
             << real << " - " << imaginaria << "i" << endl;
    }
}

int main() {
    double a, b, c;
    cout << "Ingrese coeficientes a, b, c: ";
    cin >> a >> b >> c;
    
    cuadratica(a, b, c);
    
    return 0;
}
